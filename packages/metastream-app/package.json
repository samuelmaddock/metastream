{
  "name": "metastream",
  "private": true,
  "version": "0.10.4",
  "description": "Metastream for the web browser",
  "productName": "Metastream",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack-cli --config webpack.config.prod.js",
    "locale:diff": "node scripts/localeDiff.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "start:https": "cross-env NODE_ENV=development USE_HTTPS=1 webpack-dev-server --config webpack.config.dev.js",
    "start:firefox": "cross-env PORT=80 NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "start:signal-server": "node scripts/signal-server.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:clean": "del-cli \"./test/artifacts/*\"",
    "test:unit": "jest --runInBand --verbose",
    "test:e2e": "npm run test:clean && jest --runInBand --verbose -c jest-e2e.config.js",
    "test:watch": "jest --watch --runInBand --verbose"
  },
  "browserslist": "Chrome >= 73, Firefox >= 66",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuelmaddock/metastream"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.3",
    "@types/classnames": "^2.2.3",
    "@types/deep-diff": "^1.0.0",
    "@types/history": "^4.6.1",
    "@types/i18next": "^11.9.3",
    "@types/jest": "^24.0.19",
    "@types/libsodium-wrappers": "^0.7.0",
    "@types/lodash-es": "^4.17.3",
    "@types/marked": "^0.6.5",
    "@types/node": "^12.11.7",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^4.0.8",
    "@types/react-router-redux": "^5.0.8",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.5",
    "@types/request": "^2.0.6",
    "@types/semver": "^5.5.0",
    "@types/shortid": "^0.0.29",
    "@types/simple-peer": "^6.1.6",

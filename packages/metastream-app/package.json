      "prettier --write",
      "git add"
    ]
  },
  "devEngines": {
    "node": ">=10.x",
    "yarn": ">=1.10.1"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "jsonEnable": false,
    "semi": false
  },
  "sideEffects": [
    "*.css"
  ]
}
